getgenv().collectOrbsToggle = false
getgenv().autoRebirth = false
getgenv().autoBeachBallva = false


function autoOrb()
    spawn(function()
        local playerHead = game.Players.LocalPlayer.Character.Head
        while wait(0.1) do
            if not getgenv().collectOrbsToggle then break end
            for i, v in pairs(game:GetService("Workspace").GameAssets.GlobalAssets.OrbSpawns:GetDescendants()) do
                if v.Name == "TouchInterest" and v.Parent then
                    firetouchinterest(playerHead, v.Parent, 0)
                    wait(0.1)
                    break;
                end
            end
        end
    end)
end

function autoBeachBall()
    spawn(function()
        local playerHead = game.Players.LocalPlayer.Character.Head
        while wait(0.1) do
            if not getgenv().autoBeachBallva then break end
            for i, v in pairs(game:GetService("Workspace").GameAssets.GlobalAssets.EventAssets.Summer.Collectibles:GetDescendants()) do
                if v.Name == "TouchInterest"  then
                    firetouchinterest(playerHead, v.Parent, 0)
                    wait(0.1)
                    break;
                end
            end
        end
    end)
end

function autoCollect()
    spawn(function ()
        while wait(1.0) do
            if getgenv().collectOrbsToggle == true then
                game:GetService("ReplicatedStorage").Remotes.AddSpeed:FireServer()
                wait(1.0)
            else
                print('Auto Collect Stopped')
                break
            end
        end
        
    end)
    
end


function autoRebirthva()
    spawn(function ()
        while wait(7.0) do
            if getgenv().autoRebirth == true then
                game:GetService("ReplicatedStorage").Remotes.Rebirth:FireServer()
                wait(7.0)
            else
                print('Auto Rebirth Stopped')
                break
            end
        end
        
    end)
    
end






local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/AikaV3rm/UiLib/master/Lib.lua')))()

local w = library:CreateWindow("SRS || Bebedi#9960")

local b = w:CreateFolder("Farming")

local e = w:CreateFolder("Egg")

local c = w:CreateFolder("Teleport")

local d =w:CreateFolder("Other")

b:Label("Credit:",{
    TextSize = 17; -- Self Explaining
    TextColor = Color3.fromRGB(255,255,255); -- Self Explaining
    BgColor = Color3.fromRGB(69,69,69); -- Self Explaining
    
}) 

b:Label("infantry,[<3]",{
    TextSize = 15; -- Self Explaining
    TextColor = Color3.fromRGB(255,255,255); -- Self Explaining
    BgColor = Color3.fromRGB(69,69,69); -- Self Explaining
    
}) 


Login.Name = "Login"
Login.Parent = MainLogin
Login.BackgroundColor3 = Color3.fromRGB(31, 31, 31)
Login.Position = UDim2.new(0.302013427, 0, 0.788659811, 0)
Login.Size = UDim2.new(0, 118, 0, 50)
Login.Font = Enum.Font.SourceSansBold
Login.Text = "Login"
Login.TextColor3 = Color3.fromRGB(0, 0, 0)
Login.TextSize = 43.000
Login.MouseButton1Down:Connect(function()
	if KeyBox.Text == "ejekrLEoW" then
		MainLogin.Visible = false
        getgenv().collectOrbsToggle = false
        getgenv().autoRebirth = false
        getgenv().autoBeachBallva = false
        getgenv().autoRing = false


        function autoOrb()
            spawn(function()
                local name = game.Workspace[game.Players.LocalPlayer.DisplayName]
                local playerHead = name.HumanoidRootPart
                while wait(0.1) do
                    if not getgenv().collectOrbsToggle then break end
                    for i, v in pairs(game:GetService("Workspace").GameAssets.GlobalAssets.OrbSpawns:GetDescendants()) do
                        if v.Name == "TouchInterest" and v.Parent then
                            firetouchinterest(playerHead, v.Parent, 0)
                            wait(0.1)
                            break;
                        end
                    end
                end
            end)
        end

        function autoBeachBall()
            spawn(function()
                local name = game.Workspace[game.Players.LocalPlayer.DisplayName]
                local playerHead = name.HumanoidRootPart
                while wait(0.1) do
                    if not getgenv().autoBeachBallva then break end
                    for i, v in pairs(game:GetService("Workspace").GameAssets.GlobalAssets.EventAssets.Summer.Collectibles:GetDescendants()) do
                        if v.Name == "TouchInterest"  then
                            firetouchinterest(playerHead, v.Parent, 0)
                            wait(0.1)
                            break;
                        end
                    end
                end
            end)
        end

        function autoCollect()
            spawn(function ()
                while wait(1.0) do
                    if getgenv().collectOrbsToggle == true then
                        game:GetService("ReplicatedStorage").Remotes.AddSpeed:FireServer()
                        wait(1.0)
                    else
                        print('Auto Collect Stopped')
                        break
                    end
                end
                
            end)
            
        end


        function autoRebirthva()
            spawn(function ()
                while wait(7.0) do
                    if getgenv().autoRebirth == true then
                        game:GetService("ReplicatedStorage").Remotes.Rebirth:FireServer()
                        wait(7.0)
                    else
                        print('Auto Rebirth Stopped')
                        break
                    end
                end
                
            end)
            
        end



        local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/AikaV3rm/UiLib/master/Lib.lua')))()

        local w = library:CreateWindow("Farming")

        local z = library:CreateWindow("Teleport")

        local t = library:CreateWindow("Buy")

        local y = library:CreateWindow("Race")

        local n = library:CreateWindow("Other")

        local x = library:CreateWindow("Credits")

        local b = w:CreateFolder("Farming")

        local e = t:CreateFolder("Egg")

        local c = z:CreateFolder("Teleport")

        local u = x:CreateFolder("Credits")

        local d = n:CreateFolder("Other")

        local i = y:CreateFolder("Race")

        u:Label("Credit:",{
            TextSize = 17; -- Self Explaining
            TextColor = Color3.fromRGB(255,255,255); -- Self Explaining
            BgColor = Color3.fromRGB(69,69,69); -- Self Explaining
            
        }) 

        u:Label("infantry,[<3]",{
            TextSize = 15; -- Self Explaining
            TextColor = Color3.fromRGB(255,255,255); -- Self Explaining
            BgColor = Color3.fromRGB(69,69,69); -- Self Explaining
            
        }) 

        u:Label("Script Made By:",{
            TextSize = 17; -- Self Explaining
            TextColor = Color3.fromRGB(255,255,255); -- Self Explaining
            BgColor = Color3.fromRGB(69,69,69); -- Self Explaining
            
        }) 

        u:Label("Bebedi#9960",{
            TextSize = 16; -- Self Explaining
            TextColor = Color3.fromRGB(255,255,255); -- Self Explaining
            BgColor = Color3.fromRGB(69,69,69); -- Self Explaining
            
        }) 

        e:Label("To buy Egg 2 you must have at least 2 Rebirths",{
            TextSize = 13; -- Self Explaining
            TextColor = Color3.fromRGB(255,255,255); -- Self Explaining
            BgColor = Color3.fromRGB(69,69,69); -- Self Explaining
            
        }) 

        e:Label("To buy Egg 3 you must have at least 3 Rebirths",{
            TextSize = 13; -- Self Explaining
            TextColor = Color3.fromRGB(255,255,255); -- Self Explaining
            BgColor = Color3.fromRGB(69,69,69); -- Self Explaining
            
        }) 

        e:Label("There is no animation when you buy it! See your inventory",{
            TextSize = 13; -- Self Explaining
            TextColor = Color3.fromRGB(255,255,255); -- Self Explaining
            BgColor = Color3.fromRGB(69,69,69); -- Self Explaining
            
        }) 


        b:Toggle("Auto Orb",function(bool)
            getgenv().collectOrbsToggle = bool
            print("Auto Orb is:" , bool);
            if bool then
                autoCollect()
                autoOrb()
            end
        end)


        b:Toggle("Auto BeachBall",function(bool)
            getgenv().autoBeachBallva = bool
            print("Auto Beach is:" , bool);
            if bool then
                autoBeachBall()
            end
        end)


        b:Toggle("Auto Rebirth",function(bool)
            getgenv().autoRebirth = bool
            print("Auto Rebirth is:" , bool);
            if bool then
                autoRebirthva()
            end
        end)

        b:Button("Rebirth",function()
            game:GetService("ReplicatedStorage").Remotes.Rebirth:FireServer()
        end)

        e:Button("Buy Egg 1",function()
            local args = {[1] = "EggOne",[2] = false}
            game:GetService("ReplicatedStorage").Remotes.CanBuyEgg:InvokeServer(unpack(args))
        end)

        e:Button("Buy Egg 2",function()
            local args = {[1] = "EggTwo",[2] = false}
            game:GetService("ReplicatedStorage").Remotes.CanBuyEgg:InvokeServer(unpack(args))
        end)

        e:Button("Buy Egg 3",function()
            local args = {[1] = "EggThree",[2] = false}
            game:GetService("ReplicatedStorage").Remotes.CanBuyEgg:InvokeServer(unpack(args))
        end)

        d:Button("Inf Jump",function()
            local Player = game:GetService'Players'.LocalPlayer;
        local UIS = game:GetService'UserInputService';

        _G.JumpHeight = 50;
        
        function Action(Object, Function) if Object ~= nil then Function(Object); end end
        
        UIS.InputBegan:connect(function(UserInput)
            if UserInput.UserInputType == Enum.UserInputType.Keyboard and UserInput.KeyCode == Enum.KeyCode.Space then
                Action(Player.Character.Humanoid, function(self)
                    if self:GetState() == Enum.HumanoidStateType.Jumping or self:GetState() == Enum.HumanoidStateType.Freefall then
                        Action(self.Parent.HumanoidRootPart, function(self)
                            self.Velocity = Vector3.new(0, _G.JumpHeight, 0);
                        end)
                    end
                end)
            end
        end)
        end)

        i:Button("Instant win",function()
            local name = game.Workspace[game.Players.LocalPlayer.DisplayName]
            local playerHead = name.HumanoidRootPart
            for i, v in pairs(game:GetService("Workspace").GameAssets.Races.Grassy.Course.Finish.RaceEnd:GetDescendants()) do
                if v.Name == "TouchInterest" then
                    firetouchinterest(playerHead, v.Parent, 0)
                    wait(0.1)
                    firetouchinterest(playerHead, v.Parent, 1)
                end
            end
        end)


        c:Button("Desert",function()
            function teleportTO(placeCFrame)
                local name = game.Workspace[game.Players.LocalPlayer.DisplayName]
                local plyr = name.HumanoidRootPart
                if plyr then
                    plyr.CFrame = placeCFrame
                end
            end
            teleportTO(game:GetService("Workspace").GameAssets.Maps.Desert.Map.Floor.Baseplate.CFrame)
            wait(0.3)
            teleportTO(game:GetService("Workspace").GameAssets.Maps.Desert.Map.Floor.Baseplate.CFrame)
        end)


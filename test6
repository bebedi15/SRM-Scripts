local ScreenGui = Instance.new("ScreenGui")
local MainLogin = Instance.new("Frame")
local LoginTitle = Instance.new("TextLabel")
local KeyBox = Instance.new("TextBox")
local Copydiscordlink = Instance.new("TextButton")
local Title1 = Instance.new("TextLabel")
local Title2 = Instance.new("TextLabel")
local Login = Instance.new("TextButton")


ScreenGui.Parent = game.CoreGui

MainLogin.Name = "Main Login"
MainLogin.Parent = ScreenGui
MainLogin.BackgroundColor3 = Color3.fromRGB(52, 52, 52)
MainLogin.Position = UDim2.new(0.388682067, 0, 0.252760738, 0)
MainLogin.Size = UDim2.new(0, 298, 0, 388)
MainLogin.Active = true
MainLogin.Draggable = true

LoginTitle.Name = "Login Title"
LoginTitle.Parent = MainLogin
LoginTitle.BackgroundColor3 = Color3.fromRGB(52, 52, 52)
LoginTitle.Size = UDim2.new(0, 298, 0, 60)
LoginTitle.Font = Enum.Font.SourceSansBold
LoginTitle.Text = "Login"
LoginTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
LoginTitle.TextSize = 51.000

KeyBox.Name = "KeyBox"
KeyBox.Parent = MainLogin
KeyBox.BackgroundColor3 = Color3.fromRGB(31, 31, 31)
KeyBox.BorderColor3 = Color3.fromRGB(27, 42, 53)
KeyBox.Position = UDim2.new(0.127516776, 0, 0.435567021, 0)
KeyBox.Size = UDim2.new(0, 222, 0, 50)
KeyBox.Font = Enum.Font.SourceSansBold
KeyBox.Text = "Key Here"
KeyBox.TextColor3 = Color3.fromRGB(255, 255, 255)
KeyBox.TextSize = 47.000
KeyBox.TextStrokeColor3 = Color3.fromRGB(255, 255, 255)

Copydiscordlink.Name = "Copy Link for Key"
Copydiscordlink.Parent = MainLogin
Copydiscordlink.BackgroundColor3 = Color3.fromRGB(31, 31, 31)
Copydiscordlink.Position = UDim2.new(0.184563756, 0, 0.613402069, 0)
Copydiscordlink.Size = UDim2.new(0, 189, 0, 43)
Copydiscordlink.Font = Enum.Font.SourceSansBold
Copydiscordlink.Text = "Copy Discord Link"
Copydiscordlink.TextColor3 = Color3.fromRGB(255, 255, 255)
Copydiscordlink.TextSize = 27.000
Copydiscordlink.MouseButton1Down:Connect(function()
	setclipboard("https://discord.gg/FyaBTNwxMY")
end)

Title1.Name = "Title 1"
Title1.Parent = MainLogin
Title1.BackgroundColor3 = Color3.fromRGB(52, 52, 52)
Title1.Position = UDim2.new(0, 0, 0.175257728, 0)
